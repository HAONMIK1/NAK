<%@page import="market.produser.ProdUserBean"%>
<%@page import="java.util.ArrayList"%>
<%@page import="market.produser.ProdUserDao"%>
<%@page import="market.users.UserDAO"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ include file="tradeTop.jsp"%>
<tr>
	<td width="1000" height="500">
		<%
	ProdUserDao udao =ProdUserDao.getInstance();
	
%>
		<table>
		<tr><td colspan="2"><h1> 상품 </h1></td></tr>
		<tr>
		<td width="300" align="center">
		<div>
			<img
				src="data:image/svg+xml;base64,">
		</div> <%=id %><br> 
			</td>
			
				<%
    ArrayList<ProdUserBean> lists =new ArrayList<ProdUserBean>() ; 
    lists =udao.selectID(id);
    for(int i=0;i<lists.size();i++){
		ProdUserBean pub =lists.get(i);
    	%>
				<td align="center"><a href="tradeProductsU.jsp?id=<%=id%>&tnum=<%=pub.getTnum()%>">
				<img src="../img/<%=pub.getTimg()%>" width="100"> <br>
					<br> <%=pub.getTlocation() %><br> <%=pub.getTname() %><br>
					<%=pub.getTprice() %>
				</a>
					</td>
				<%if((i+1)%3==0){ %>
			</tr>
			<%} }%>
			</tr>
		<td>
		</tr>
		</table>

	</td>
</tr>
<jsp:include page="../home/bottom.jsp" />